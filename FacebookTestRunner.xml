<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Facebook Automation Script " parallel="false">
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="69.0.3497.100"/>
    <parameter name="url" value="https://www.facebook.com/"/>
    <test name = "Facebook Test case">
        <classes>
            <class name="search.TestFacebookLogIn"/>
            <class name="search.TestNewFacebookSignUp"/>
            <class name="search.TestHomePageClick"/>
            <class name="search.TestHomeLogIn"/>
            <class name="search.TestHomePageInputData"/>
        </classes>
    </test>
</suite>